<%= form_tag user_sessions_path, :method => :post, :id => "user-session-form" do %>
  <div class="field">
    <%= text_field_tag :email, nil, :placeholder => "Email" %>
  </div>
  <div class="field">
    <%= password_field_tag :password, nil, :placeholder => "Password" %>
  </div>
  <div class="actions">
    <%= submit_tag 'Go', :class => 'btn-contact' %>
  </div>
<% end %>